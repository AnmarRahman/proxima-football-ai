{
    "version": 2,
    "builds": [
        {
            "src": "python/main.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.13",
                "maxLambdaSize": "50mb",
                "installCommand": "pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "python/main.py"
        }
    ]
}